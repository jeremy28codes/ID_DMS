@model IEnumerable<DMS.Models.System_users>

<thead>
    <tr>
        <th>
            ID
        </th>
        <th>
            Reference Number
        </th>
        <th>
            Name
        </th>
        <th>
            Job Title
        </th>
        <th>
            Office
        </th>
        <th>
            Division
        </th>
        <th width="15%">Actions</th>
    </tr>
</thead>

@if (Model.Count() > 0)
{
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.reference_number)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.last_name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.job_title)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.department_code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.division_code)
                </td>
                <td style="text-align: center;">
                    <a id="sa_edit" href="javascript:void(0);" data-id="@item.id" class="btn btn-primary">
                        <i class="fas fa-pencil-alt"></i>
                    </a>
                    <a id="sa_delete" href="javascript:void(0);" data-id="@item.id" class="btn btn-danger btn-primary">
                        <i class="fas fa-trash"></i>
                    </a>
                </td>
            </tr>
        }
    </tbody>

}
else
{
    @*<tbody>
            <tr class="odd text-center"><td valign="top" colspan="5" class="dataTables_empty">No data available in table</td></tr>
        </tbody>*@
}
