@model IEnumerable<DMS.Models.System_user_role_module_access>
<thead>
    <tr>
        <th>
            Web Module
        </th>
        <th>
            Can Access
        </th>
        <th>
            Can View
        </th>
        <th>
            Can Create
        </th>
        <th>
            Can Edit
        </th>
        <th>
            Can Delete
        </th>
        @*<th width="15%">Actions</th>*@
    </tr>
</thead>

@if (Model.Count() > 0)
{
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.system_web_module_name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.can_access)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.can_view)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.can_create)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.can_edit)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.can_delete)
                </td>
                @*<td style="text-align: center;">
                        <a id="ma_edit" href="javascript:void(0);" data-id="@item.id" class="btn btn-primary">
                            <i class="fas fa-pencil-alt"></i>
                        </a>
                        <a id="ma_delete" href="javascript:void(0);" data-id="@item.id" class="btn btn-danger btn-primary">
                            <i class="fas fa-trash"></i>
                        </a>
                    </td>*@
            </tr>
        }
    </tbody>

}
else
{
    @*<tbody>
        <tr class="odd text-center"><td valign="top" colspan="5" class="dataTables_empty">No data available in table</td></tr>
    </tbody>*@
}
